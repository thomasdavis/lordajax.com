{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "globalEnv": ["TOT", "DATABASE_URL", "GITHUB_TOKEN", "OPENAI_API_KEY", "PINECONE_API_KEY", "PINECONE_ENVIRONMENT"]
}
